    "test": "mocha --compilers js:babel-core/register"
  },
  "dependencies": {
    "nearley": "latest",
    "immutable": "3.8.1"
  },
  "devDependencies": {
    "babel-cli": "*",
    "babel-preset-es2015-node4": "*",
    "chai": "*",
    "coveralls": "*",
    "isparta": "*",
    "mocha": "*",
    "sinon": "*"
  }
}
